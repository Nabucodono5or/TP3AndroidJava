<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.daenerys.tp3androidjava.InfoActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="151dp"
        android:layout_marginTop="39dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textColor="@color/primaryText"/>

    <TextView
        android:id="@+id/textSenha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="44dp"
        android:text="TextView"
        android:textColor="@color/secondaryText"/>

    <TextView
        android:id="@+id/textEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textSenha"
        android:layout_below="@+id/textSenha"
        android:layout_marginTop="28dp"
        android:text="TextView"
        android:textColor="@color/secondaryText"/>

    <TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textEmail"
        android:layout_below="@+id/textEmail"
        android:layout_marginTop="28dp"
        android:text="TextView"
        android:textColor="@color/secondaryText"/>

    <TextView
        android:id="@+id/textCel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/text"
        android:layout_below="@+id/text"
        android:layout_marginTop="25dp"
        android:text="TextView"
        android:textColor="@color/secondaryText"/>

    <TextView
        android:id="@+id/textCid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textCel"
        android:layout_below="@+id/textCel"
        android:layout_marginTop="37dp"
        android:text="TextView"
        android:textColor="@color/secondaryText"/>

    <TextView
        android:id="@+id/textCpf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textCid"
        android:layout_below="@+id/textCid"
        android:layout_marginTop="34dp"
        android:text="TextView"
        android:textColor="@color/secondaryText"/>
</RelativeLayout>
